	partition tienda = m
		mode: import
		source =
				let
				    Origen = Sql.Database(srvName, dbName, [Query="SELECT #(lf)    s.StoreKey,#(lf)    s.StoreCode AS CodigoTienda,#(lf)    s.Description AS NombreTienda,#(lf)    s.SquareMeters AS MetrosCuadrados,#(lf)    s.Status AS EstadoTienda,#(lf)    st.StateName AS Estado,#(lf)    co.CountryName AS Pais#(lf)FROM [Data].Store s#(lf)LEFT JOIN [Data].State st ON s.StateKey = st.StateKey#(lf)LEFT JOIN [Data].Country co ON st.CountryCode = co.CountryCode;"])
				in
				    Origen

