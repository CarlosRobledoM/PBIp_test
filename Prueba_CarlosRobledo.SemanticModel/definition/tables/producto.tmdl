	partition producto = m
		mode: import
		source =
				let
				    Origen = Sql.Database(srvName, dbName, [Query="SELECT #(lf)    p.ProductKey,#(lf)    p.ProductCode AS CodigoProducto,#(lf)    p.ProductName AS NombreProducto,#(lf)    p.Manufacturer AS Fabricante,#(lf)    p.Brand AS Marca,#(lf)    p.Color,#(lf)    p.Weight AS PesoOriginal,#(lf)    p.WeightUnit AS UnidadPesoOriginal,#(lf)    CASE #(lf)        WHEN p.WeightUnit = 'ounces' THEN p.Weight * 0.0283495#(lf)        ELSE p.Weight#(lf)    END AS PesoKilogramos,#(lf)    p.SubCategoryKey,#(lf)    p.CategoryKey,#(lf)    sc.SubCategoryName AS SubCategoria,#(lf)    c.CategoryName AS Categoria#(lf)FROM [Data].Product p#(lf)LEFT JOIN [Data].SubCategory sc ON p.SubCategoryKey = sc.SubCategoryKey#(lf)LEFT JOIN [Data].Category c ON p.CategoryKey = c.CategoryKey;"])
				in
				    Origen

