	partition Calendario = calculated
		mode: import
		source =
				
				VAR MinYear = YEAR(MIN('Ventas'[FechaOrden]))
				VAR MaxYear = YEAR(MAX('Ventas'[FechaOrden]))
				RETURN
				ADDCOLUMNS (
				    CALENDAR(DATE(MinYear, 1, 1), DATE(MaxYear, 12, 31)),
				    "Año", YEAR([Date]),
				    "MesNumero", MONTH([Date]),
				    "Mes", FORMAT([Date], "MMMM"),
				    "Trimestre", "T" & FORMAT([Date], "Q"),
				    "DíaSemana", FORMAT([Date], "dddd")
				)

